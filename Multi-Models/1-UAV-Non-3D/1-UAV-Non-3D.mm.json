{"fmus":{
		"{uav}":"UAV_Rep.fmu",
		"{uavCont}":"UAVCon_Rep.fmu",
		"{uavGlobalCont}":"UAVGlobalController.fmu"
	},
	"connections":{
    "{uavGlobalCont}.global.uav1TargetX":["{uavCont}.controller.targetX"],
	  "{uavGlobalCont}.global.uav1TargetY":["{uavCont}.controller.targetY"],
	  "{uavGlobalCont}.global.uav1TargetZ":["{uavCont}.controller.targetZ"],
		"{uav}.uav.velX":["{uavCont}.controller.velXIn"],
		"{uav}.uav.velY":["{uavCont}.controller.velYIn"],
		"{uav}.uav.velZ":["{uavCont}.controller.velZIn"],
		"{uav}.uav.currentYaw":["{uavCont}.controller.yawIn"],
		"{uav}.uav.batteryCharge":["{uavCont}.controller.batteryChargeIn"],
		"{uavCont}.controller.pitchOut":["{uav}.uav.pitch"],
		"{uavCont}.controller.rollOut":["{uav}.uav.roll"],
		"{uavCont}.controller.yawOut":["{uav}.uav.yaw"],
		"{uavCont}.controller.throttleOut":["{uav}.uav.throttle"]
	},
	"parameters":{}
}
